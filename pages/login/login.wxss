.msg{
  width: 290rpx;
  height: 120rpx;
  margin: 0 auto;
  margin-top: 260rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.icon{
  background-color: #C4C4C4;
}
.icon>image{
  width: 120rpx;
  height: 120rpx;
}
.text{
  width: 144rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.text>view:nth-of-type(1){
  width: 144rpx;
  height: 50rpx;
  font-size: 36rpx;
}
.text>view:nth-of-type(2){
  width: 100rpx;
  height: 46rpx;
  font-size: 20rpx;
  text-align: center;
}
.wx{
  padding: 0;
  margin-top: 212rpx;
  background-color: #07C160;
  color: white;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 120rpx;
}
.ph{
  padding: 0;
  margin-top: 60rpx;
  background-color: #F1F4F3;
  color: black;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 120rpx;
}