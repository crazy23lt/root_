/* components/es-picker/es-picker.wxss */
.page {
  width: 100%;
  height: 64rpx;
}
.picker-item {
  width: 100%;
  height: 100%;
}
.icon {
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  object-fit: cover;
}
.picker {
  width: 100%;
  position: relative;
  height: 64rpx;
  text-align: center;
  line-height: 64rpx;
}
